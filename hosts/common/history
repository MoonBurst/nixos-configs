: 1762774290:0;nupdate() {\
  local HOSTNAME\
  HOSTNAME=$(hostname)\
  # ALTERNATIVE FIX: Remove quotes to allow tilde expansion\
  local FLAKE_PATH=~/nixos-config \
\
  if [ -d "$FLAKE_PATH" ]; then\
    echo "Building system: $HOSTNAME from $FLAKE_PATH"\
\
    sudo nixos-rebuild switch --flake "$FLAKE_PATH"\#"$HOSTNAME"\
  else\
    echo "Error: Flake path not found at $FLAKE_PATH" >&2\
    return 1\
  fi\
}

nix-shell -p pciutils nvme-cli
PORT=$(basename $(dirname $(readlink "/sys/bus/pci/devices/0000:25:00.0")))\
echo "Upstream Port: $PORT"\

nix-shell -p pciutils --run '\
  DEV="0000:25:00.0"\
  PORT_PATH=$(readlink -f /sys/bus/pci/devices/$DEV/..)\
  PORT=$(basename $PORT_PATH)\
  echo "Target Device: $DEV"\
  echo "Upstream Port: $PORT"\
  \
  # Step 2: Remove the device from the OS\
  echo 1 | sudo tee /sys/bus/pci/devices/$DEV/remove\
  \
  # Step 3: Trigger Secondary Bus Reset (Hot Reset)\
  # This flips bit 0x40 in the BRIDGE_CONTROL register\
  BC=$(setpci -s $PORT BRIDGE_CONTROL)\
  sudo setpci -s $PORT BRIDGE_CONTROL=$(printf "%04x" $((0x$BC | 0x40)))\
  sleep 0.01\
  sudo setpci -s $PORT BRIDGE_CONTROL=$BC\
  sleep 0.5\
  \
  # Step 4: Rescan the bus\
  echo 1 | sudo tee /sys/bus/pci/rescan\
'\

# Force a rescan of the entire bus first\
echo 1 | sudo tee /sys/bus/pci/rescan\
\
# Manually trigger the Secondary Bus Reset on the bridge (replace XX:XX.X)\
sudo nix-shell -p pciutils --run 'setpci -s XX:XX.X BRIDGE_CONTROL=0x40; sleep 0.1; setpci -s XX:XX.X BRIDGE_CONTROL=0x00'\
\
# Rescan again\
echo 1 | sudo tee /sys/bus/pci/rescan\

nix-shell -p pciutils --run "lspci -tv"\

nix-shell -p pciutils --run "\
  sudo setpci -s 20:00.2 BRIDGE_CONTROL=0x40; \
  sleep 0.2; \
  sudo setpci -s 20:00.2 BRIDGE_CONTROL=0x00; \
  echo 1 | sudo tee /sys/bus/pci/rescan\
"\

